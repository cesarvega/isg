{
  "npmScope": "nx",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "earthlink": {
      "tags": []
    },
    "earthlink-address": {
      "tags": []
    },
    "earthlink-api": {
      "tags": []
    },
    "earthlink-config": {
      "tags": []
    },
    "earthlink-e2e": {
      "tags": [],
      "implicitDependencies": ["earthlink"]
    },
    "earthlink-env": {
      "tags": []
    },
    "earthlink-login": {
      "tags": []
    },
    "earthlink-logout": {
      "tags": []
    },
    "earthlink-offers": {
      "tags": []
    },
    "earthlink-shared": {
      "tags": []
    },
    "earthlink-utilities": {
      "tags": []
    }
  }
}
